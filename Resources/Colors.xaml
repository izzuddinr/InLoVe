<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="StartColor" Color="#4E8752"/>
    <SolidColorBrush x:Key="StopColor" Color="#B54747"/>
    <SolidColorBrush x:Key="VerboseColor" Color="#FCFCFC"/>
    <SolidColorBrush x:Key="DebugColor" Color="#BDDEE7"/>
    <SolidColorBrush x:Key="InfoColor" Color="#DEE6C2"/>
    <SolidColorBrush x:Key="WarningColor" Color="#FFE15C"/>
    <SolidColorBrush x:Key="ErrorColor" Color="#FF5C7A"/>
    <SolidColorBrush x:Key="FatalColor" Color="#B8002B"/>

    <SolidColorBrush x:Key="AppBackgroundColor" Color="#201e23"/>

</ResourceDictionary>
